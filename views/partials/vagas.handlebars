 <section class="section-estacionamento mt-16">
            <div class="row mt-16 mb-5">
                {{#each setores}}
                <div class="col-12 col-md-6 mt-3">
                    <h3 class="titulo_vaga">Setor {{@key}}</h3>

                    <div class="row g-3">
                        {{#each this}}
                        <div class="col-12 col-md-6">
                            <div class="vaga_status {{status}}">
                                <div class="vaga">
                                    <span>#{{numero}}</span>
                                </div>
                                <p>{{#if (eq status "disponivel")}}Dispon√≠vel{{else}}Ocupado{{/if}}</p>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
                {{/each}}
            </div>
        </section>
